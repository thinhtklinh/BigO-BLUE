# Ex4: GukiZ and Contest -> O(nlogn)
read(n)
read(arr)

Sarr = sorted(arr)
int[2001] rank

for i = 0 to n-1:
    if (rank[Sarr[i]] == 0):
        rank[Sarr[i]] = i+1
    
for i = 0 to n-1:
    print(rank[arr[i]])


